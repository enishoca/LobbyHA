{
  "name": "lobbyha",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev:server & npm run dev:dashboard",
    "dev:server": "npm run dev -w @lobbyha/server",
    "dev:dashboard": "npm run dev -w @lobbyha/dashboard",
    "build": "npm run build -w @lobbyha/dashboard",
    "start": "npm run start -w @lobbyha/server",
    "setup": "npm install --legacy-peer-deps && npm run build"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.4.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fullcalendar/react": "^6.1.20",
    "@iconify/react": "^6.0.2",
    "@use-gesture/react": "^10.3.1",
    "autolinker": "^4.1.5",
    "fullcalendar": "^6.1.20",
    "hls.js": "^1.6.15",
    "home-assistant-js-websocket": "^9.6.0",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "react-dom": "^19.2.3",
    "react-error-boundary": "^5.0.0",
    "react-leaflet": "^5.0.0",
    "react-resize-detector": "^12.3.0",
    "react-switch": "^7.1.0",
    "react-use": "^17.6.0",
    "use-debounce": "^10.1.0",
    "use-long-press": "^3.3.0",
    "zustand": "^5.0.10"
  }
}
